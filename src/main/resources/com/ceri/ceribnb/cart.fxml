<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.ceri.ceribnb.CartController">
    <children>
        <ListView fx:id="cartListView" />
        <Button fx:id="validateOrderButton" text="Valider la commande" onAction="#validateOrder" />
        <Button fx:id="closeButton" text="fermer" onAction="#close" />
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>
</VBox>
